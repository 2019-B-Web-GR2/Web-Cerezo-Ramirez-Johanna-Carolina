<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Nombre</th>
        <th scope="col">Cedula</th>
        <th scope="col">Acciones</th>
    </tr>
    </thead>
    <tbody>
    <% usuarios.forEach( (usuario) => {%>
         <tr>
        <th scope="row"><%= usuario.id%></th>
        <td><%= usuario.nombre%></td>
        <td><%= usuario.cedula%></td>
        <td>
            <button type="button"
                    class="btn btn-danger">
                Editar
            </button>
            <button  type="button"
                     class="btn btn-danger">
                Eliminar
            </button>
        </td>
    </tr>
    <%}); %>
    </tbody>
</table>
